  <wsdl:portType name="ChannelManagerSoap">
    <wsdl:operation name="CheckAvailability">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Returns availability for the specified hotel and specified period.</wsdl:documentation>
      <wsdl:input message="tns:CheckAvailabilitySoapIn" />
      <wsdl:output message="tns:CheckAvailabilitySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetModifiedRates">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Returns rates that were modified in the specified period&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;GetModifiedRatesRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;GetModifiedRatesRequest.ModifiedDateFrom&lt;/b&gt; - the start date of the modification period&lt;/li&gt;
&lt;li&gt;&lt;b&gt;GetModifiedRatesRequest.ModifiedDateTill&lt;/b&gt; - the end date of the modification period&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:GetModifiedRatesSoapIn" />
      <wsdl:output message="tns:GetModifiedRatesSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetProducts">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Gets all products owned by hotel</wsdl:documentation>
      <wsdl:input message="tns:GetProductsSoapIn" />
      <wsdl:output message="tns:GetProductsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetProperties">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Returns all available hotels for the specified user credentials.</wsdl:documentation>
      <wsdl:input message="tns:GetPropertiesSoapIn" />
      <wsdl:output message="tns:GetPropertiesSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetRateInfo">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Returns rate and availability data&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;GetRateInfoRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;GetRateInfoRequest.RoomId&lt;/b&gt; - id of the room from &lt;i&gt;GetRooms&lt;/i&gt; response&lt;/li&gt;
&lt;li&gt;&lt;b&gt;GetRateInfoRequest.RatePlanId&lt;/b&gt; - id of the rate plan from &lt;i&gt;GetRatePlans&lt;/i&gt; response&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:GetRateInfoSoapIn" />
      <wsdl:output message="tns:GetRateInfoSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="GetSupplements">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Returns all supplements for the specified hotel.</wsdl:documentation>
      <wsdl:input message="tns:GetSupplementsSoapIn" />
      <wsdl:output message="tns:GetSupplementsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="IncreaseRate">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Increase or decrease rates or allotment for specified room, rate plan and date period.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateRequest.RoomId&lt;/b&gt; - id of the room from &lt;i&gt;GetRooms&lt;/i&gt; response&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateRequest.RatePlanId&lt;/b&gt; - id of the rate plan from &lt;i&gt;GetRatePlans&lt;/i&gt; response&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateRequest.PriceDelta, AllotmentDelta&lt;/b&gt; - Increase price or allotment.&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:IncreaseRateSoapIn" />
      <wsdl:output message="tns:IncreaseRateSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="IncreaseRateForSingleDateRange">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Increase or decrease rates or allotment for multiple rooms, multiple rate plans and single date period.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateForSingleDateRangeRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateForSingleDateRangeRequest.RoomIds&lt;/b&gt; - array of room identifiers&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateForSingleDateRangeRequest.RatePlanIds&lt;/b&gt; - array of rate plans identifiers&lt;/li&gt;
&lt;li&gt;&lt;b&gt;IncreaseRateForSingleDateRangeRequest.PriceDelta, AllotmentDelta&lt;/b&gt; - Increase price or allotment.&lt;/li&gt;
&lt;li&gt;The rate will be applied for all combinations of room/ratePlan pairs. If 'RoomIds' and 'RatePlanIds' are nulls (but not empty) then rate will be applied for entire hotel for specified date period.&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:IncreaseRateForSingleDateRangeSoapIn" />
      <wsdl:output message="tns:IncreaseRateForSingleDateRangeSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="InsertOrder">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Inserts new hotel order.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.NewOrder.PartnerId&lt;/b&gt; - the same value as returned in CheckedAvailability, in &lt;i&gt;AvailableRoom.PartnerId&lt;/i&gt; field&lt;/li&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.NewOrder.Code&lt;/b&gt; - unique identifier of a reservation. Max length is 10 chars. If empty then it will automatically assigned. If a reservation with specified code already exists then it is overwritten&lt;/li&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.NewOrder.Status&lt;/b&gt; - Submitted: not yet confirmed to the customer, Confirmed: confirmed to the customer, Canceled, Modified: self explanatory&lt;/li&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.NewOrder.Rooms.RoomId&lt;/b&gt; - booked room identifier. You  get current room identifiers by executing &lt;i&gt;GetRooms&lt;/i&gt; method&lt;/li&gt;
&lt;li&gt;&lt;b&gt;InsertOrderRequest.NewOrder.Rooms.RatePlanId&lt;/b&gt; - booked rate plan identifier. You get current rate plans identifiers by executing &lt;i&gt;GetRatePlans&lt;/i&gt; method&lt;/li&gt;
&lt;/ul&gt;
&lt;br&gt;Allotment of the room will be reduced if status is 'Confirmed', increased in case of status 'Canceled'.
</wsdl:documentation>
      <wsdl:input message="tns:InsertOrderSoapIn" />
      <wsdl:output message="tns:InsertOrderSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="CancelOrder">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Cancels the existing reservation.&lt;/b&gt;</wsdl:documentation>
      <wsdl:input message="tns:CancelOrderSoapIn" />
      <wsdl:output message="tns:CancelOrderSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ManageHotel">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Manages the hotel data.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRequest.Credentials&lt;/b&gt; - valid hotel credentials for &lt;i&gt;Update&lt;/i&gt; or &lt;i&gt;Delete&lt;/i&gt; operation. For &lt;i&gt;Insert&lt;/i&gt; operation use another, admin credentials provided by &lt;i&gt;support@yieldplanet.com&lt;/i&gt;&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRequest.Hotel.UserEmail&lt;/b&gt; - used only at &lt;i&gt;Insert&lt;/i&gt; operation: the &lt;i&gt;username&lt;/i&gt; of the hotel credentials. Should be a valid email address&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRequest.Hotel.UserPassword&lt;/b&gt; - used only at &lt;i&gt;Insert&lt;/i&gt; operation: the &lt;i&gt;password&lt;/i&gt; of the hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelResponse.HotelId&lt;/b&gt; - after &lt;i&gt;Insert&lt;/i&gt; operation, returns id of the hotel, that should be pass by &lt;i&gt;ManageHotelRequest.Hotel.Id&lt;/i&gt; and by &lt;i&gt;ManageHotelRequest.Credentials.HotelId&lt;/i&gt; for &lt;i&gt;Update&lt;/i&gt; or &lt;i&gt;Delete&lt;/i&gt; operation&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelResponse.TOLHotelCode&lt;/b&gt; - the code of the hotel that is returned at the booking engine at &lt;i&gt;http://ha.yieldplanet.com/xmlRes/ws.asmx&lt;/i&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:ManageHotelSoapIn" />
      <wsdl:output message="tns:ManageHotelSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ManageHotelRatePlan">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Manages the hotel rate plans.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRatePlanRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRatePlanResponse.RatePlanId&lt;/b&gt; - after &lt;i&gt;Insert&lt;/i&gt; operation, returns id of the room, that should be set to &lt;i&gt;ManageHotelRatePlanRequest.RatePlan.Id&lt;/i&gt; for &lt;i&gt;Update&lt;/i&gt; or &lt;i&gt;Delete&lt;/i&gt; operation&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:ManageHotelRatePlanSoapIn" />
      <wsdl:output message="tns:ManageHotelRatePlanSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ManageHotelRoom">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Manages the hotel rooms.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRoomRequest.Credentials&lt;/b&gt; - valid hotel credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;ManageHotelRoomResponse.RoomId&lt;/b&gt; - after &lt;i&gt;Insert&lt;/i&gt; operation, returns id of the room, that should be set to &lt;i&gt;ManageHotelRoomRequest.Room.Id&lt;/i&gt; for &lt;i&gt;Update&lt;/i&gt; or &lt;i&gt;Delete&lt;/i&gt; operation&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
      <wsdl:input message="tns:ManageHotelRoomSoapIn" />
      <wsdl:output message="tns:ManageHotelRoomSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetMultipleOccupancyRates">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">&lt;b&gt;Sets rate, availability and restrictions for multiple hotels, rooms, rate plans and date periods.&lt;/b&gt;
&lt;ul&gt;
&lt;li&gt;&lt;b&gt;SetRateRequest.Credentials&lt;/b&gt; - valid user credentials&lt;/li&gt;
&lt;li&gt;&lt;b&gt;SetRateRequest.Rates&lt;/b&gt; - an array of rate units to be set&lt;/li&gt;
&lt;/ul&gt;
</wsdl:documentation>
